<%-
  include('partials/head', {
    head : head 
  });
%>
<body class="page">
  

  <nav><%- include('partials/topnav') %></nav>

  <% if(typeof breadcrumb !== 'undefined') { %>
    <%- include('partials/breadcrumb', breadcrumb) %>
  <% } %>

  <div class="container-xl pt-4 pb-4">
    <div class="row">
      <div class="col">
        <%- body %>
      </div>
    </div>
  </div>

  <% if(typeof contactForm !== 'undefined' && contactForm) { %>
    <div class="container-fluid" style="background:#f3f3f3">
      <% contacformElementID = (contactForm.id) ?  contactForm.id : 'contactform' %>
      <div class="container-xl pt-5 pb-5" id ="<%- contacformElementID %>">
          <% if(contactForm.title) { %>
            <h2><%- contactForm.title %></h2>
          <% } else { %>
          <h2>Ota yhtett√§</h2>
          <% } %>
          <%- include('partials/contact-form', contactForm) %>
      </div>
    </div>
  <% } %>

  <div class="container-xl mt-5">
    <%- include('partials/footer') %>
  </div>

</body>
</html>